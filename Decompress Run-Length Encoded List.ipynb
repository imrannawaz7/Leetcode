    "def decompressRLElist(nums):\n",
    "    i = 0\n",
    "    res = []\n",
    "    while i < len(nums)-1:\n",
    "        res.extend(nums[i]*[nums[i+1]])\n",
    "        i+=2\n",
    "    return res"
   ]
