    "def createTargetArray(nums, index):\n",
    "    lst = []\n",
    "    j=0\n",
    "    while j < len(nums):\n",
    "        for i in index:\n",
    "            lst.insert(i,nums[j])\n",
    "            j += 1\n",
    "    return lst\n",
    "            "
   ]
