    "def removeDuplicates(S):\n",
    "    stack = []\n",
    "    for s in S:\n",
    "        if stack and stack[-1] == s:\n",
    "            stack.pop()\n",
    "        else:\n",
    "            stack.append(s)\n",
    "    return ''.join(stack)\n"
   ]
