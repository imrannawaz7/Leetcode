    "def countNegatives(grid):\n",
    "    count=0\n",
    "    grid = sorted(grid)\n",
    "    for i in grid:\n",
    "        for j in i:\n",
    "            if j < 0:\n",
    "                count+=1\n",
    "    return count"
   ]
