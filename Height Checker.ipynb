    "def heightChecker(heights):\n",
    "    copy = sorted(heights)\n",
    "    count = 0\n",
    "    for i in range(len(copy)):\n",
    "        if copy[i] != heights[i]:\n",
    "            count +=1\n",
    "    return count"
   ]
