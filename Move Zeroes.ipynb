    "def moveZeroes(nums):\n",
    "    count = nums.count(0)\n",
    "    while 0 in nums:\n",
    "        nums.remove(0)\n",
    "    \n",
    "    for i in range(count):\n",
    "        nums.append(0)\n",
    "    return nums"
   ]
